var AnswerValueOther=&apos;&apos;; &#47;&#47; Answer Value which corresponds to other types

if (QuestionDataType == &apos;NUMBER&apos;)
	AnswerValueNumber = AnswerValue;
else if (QuestionDataType == &apos;POSITIVEFLOAT&apos;)
	AnswerValuePositiveFloat = AnswerValue;
else if (QuestionDataType == &apos;POSITIVEINTEGER&apos;)

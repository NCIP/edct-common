  <step>
    <name>Get Question Short Name (table questions)</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>hcit_FormBuilder</connection>
    <rowlimit>1</rowlimit>
    <sql>select q.short_name as &quot;QuestionShortName&quot;,
       sn2.client_sn as &quot;QuestionText&quot;
       from form_element fe, question q, 
	   fileprocess.sn_mapping sn, fileprocess.sn_mapping sn2, answer a
       left join answer_value av on av.answer_id=a.id
       where fe.id = q.parent_id and a.question_id = q.id 
       and fe.table_type is not null
       and sn.hcit_sn = fe.table_short_name
       and sn2.hcit_sn = q.short_name
       and sn.client_sn = ?
       -- Only include FIELDORDER in the join for STATIC and DYNAMIC tables
       and (case fe.table_type when &apos;SIMPLE&apos; then true else 
		(case fe.table_type when &apos;SIMPLE&apos; then 1 else q.ord end = ?) end)
       -- ONLY include RECORDORDER in the join for SIMPLE tables
       and (case fe.table_type when &apos;DYNAMIC&apos; then true when &apos;STATIC&apos; then true else
		(case fe.table_type when &apos;SIMPLE&apos; then q.ord else 999999 end = ?) end)</sql>
    <outer_join>N</outer_join>
    <replace_vars>N</replace_vars>
    <parameter>
      <field>
        <name>TableQuestionText</name>
        <type>String</type>
